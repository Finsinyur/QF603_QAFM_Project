    "### DollarHedge - Analysing Dollar Neutral Hedge Arbitrage"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 161,
   "id": "986ccbb8",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_rets = ret_dollar_neutral.copy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 162,
   "id": "f0bce9e3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def run_grannular_t_test(df_rets):\n",
    "    t_test_res = []\n",
    "    for series in df_rets.columns[1:]:\n",
    "        returns = np.array(df_rets[series])\n",
    "        mean_ret = returns.mean()\n",
    "        t_statistic, p_value = stats.ttest_1samp(returns, 0)\n",
    "\n",
    "        t_test_res.append({\n",
    "            'series': series,\n",
    "            'group': 'val',\n",
    "            'mean': mean_ret,\n",
    "            't-stats' : t_statistic,\n",
    "            'p-value' : p_value,\n",
    "            'conclusion' : 'reject Null' if p_value < 0.05 else 'do not reject Null'\n",
    "        })\n",
    "\n",
    "    df_t_test_granular = pd.DataFrame(t_test_res)\n",
    "    return df_t_test_granular"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 163,
   "id": "ad536be7",
   "metadata": {},
   "outputs": [],
   "source": [
    "df_t_test_granular_overall = run_grannular_t_test(df_rets)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 164,
   "id": "e67bac42",
   "metadata": {},
   "outputs": [],
