   "source": [
    "look_back = 40\n",
    "look_forward = 40\n",
    "\n",
    "ftse_rebal[\"Post Date\"] =\\\n",
    "    pd.to_datetime(ftse_rebal[\"Post Date\"], \n",
    "                   format = '%d/%m/%Y')\n",
    "\n",
    "ftse_rebal[\"start_date\"] =\\\n",
    "    (\n",
    "        ftse_rebal[\"Post Date\"] - timedelta(days = look_back)\n",
    "    ).dt.strftime('%d/%m/%Y')\n",
    "\n",
    "ftse_rebal[\"end_date\"] =\\\n",
    "    (\n",
    "        ftse_rebal[\"Post Date\"] + timedelta(days = look_back)\n",
    "    ).dt.strftime('%d/%m/%Y')\n",
    "\n",
    "ftse_rebal"
   ]
