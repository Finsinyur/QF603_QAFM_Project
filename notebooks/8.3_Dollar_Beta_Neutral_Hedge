   "cell_type": "code",
   "execution_count": 15,
   "id": "9fb7882f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Remove stocks that are suspended from trading during analysis period\n",
    "suspended_stocks = [('GB00BJP5HK17', '2022Q4'), ('GB00B1VNST91', '2018Q2'), ('GB0007892358', '2017Q2')]\n",
    "\n",
    "mask =\\\n",
    "    ~lse_historical_prices\\\n",
    "        .apply(lambda x: (x['isin_code'], x['rebal']) in suspended_stocks, axis = 1)\n",
    "\n",
    "\n",
    "lse_historical_prices = lse_historical_prices[mask]"
   ]
  },
